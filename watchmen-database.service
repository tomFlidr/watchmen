[Unit]
Description=Watchman Database Service
After=network.target
After=network-online.target
Wants=network-online.target

[Service]
Type=notify
User=redis
Group=redis
# RuntimeDirectory=redis
# RuntimeDirectoryMode=0755

ExecStart=/var/www/html/watchmen/run-database-server.sh
WorkingDirectory=/var/www/html/watchmen

[Install]
WantedBy=multi-user.target
